// initialize procedure
proc wello() {
  print("wello");
}

// procedure with return
proc withReturn(): int {
  return 20 + 33;
}

// procedure with parameter
proc withParameter(name: string): string {
  return "wello " + name;
}

// call procedures
wello();
print(withReturn());
print(withParameter("arfy"));

// Test procedure with no return value
proc greet(name: string) {
  print("Hello, " + name);
}

greet("World");
