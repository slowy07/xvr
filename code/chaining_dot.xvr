// create custom procedure that works with specific 
// variable
proc _identity(self) {
  return self;
}

// make simple function if value are same as check on
// procedure
proc _check(self) {
  assert self == 42, "dot chaining error";
  return self;
}

var value: int = 42;

// make dot chaining
value._identity()._check()._identity()._check();

// create custom procedure for increment that
// works on variable
proc _increment(self) {
  return self + 1;
}

// test all
assert 3._increment()._increment() == 5, "dot chaining increment failed";
var angka: int = 2;
if (angka._increment() == 3) {
  print("odd");
} else {
  print("even");
}

